{
  "rewrites": [
    {
      "source": "/app/",
      "destination": "https://globepick-backend-production.up.railway.app/app/"
    },
    {
      "source": "/app/:path*",
      "destination": "https://globepick-backend-production.up.railway.app/app/:path*"
    },
    {
      "source": "/.well-known/apple-app-site-association",
      "destination": "https://globepick-backend-production.up.railway.app/.well-known/apple-app-site-association"
    }
  ]
}
