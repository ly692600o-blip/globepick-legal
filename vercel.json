{
  "rewrites": [
    {
      "source": "/app/",
      "destination": "https://globepick-backend-production.up.railway.app/app/"
    },
    {
      "source": "/app/:path*",
      "destination": "https://globepick-backend-production.up.railway.app/app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}
